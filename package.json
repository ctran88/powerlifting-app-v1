{
  "name": "the-powerlifting-notebook",
  "version": "1.0.0",
  "description": "A powerlifting application for coaches and clients to communicate all of their needs.",
  "dependencies": {
    "firebase-tools": "^3.9.1",
    "vue-cli": "^2.8.2"
  },
  "scripts": {
    "uninstall:all": "npm uninstall $(ls -1 node_modules | tr '\n' ' ')",
    "vue-cli:init": "./node_modules/.bin/vue-cli init webpack app",
    "firebase:login": "./node_modules/.bin/firebase login",
    "firebase:deploy": "./node_modules/.bin/firebase deploy",
    "firebase:hosting": "./node_modules/.bin/firebase deploy --only hosting",
    "firebase:functions": "./node_modules/.bin/firebase deploy --only functions",
    "firebase:database": "./node_modules/.bin/firebase deploy --only database",
    "firebase:storage": "./node_modules/.bin/firebase deploy --only storage",
    "firebase:serve": "./node_modules/.bin/firebase serve --only functions,hosting",
    "firebase:get-configs": "./node_modules/.bin/firebase functions:config:get > .runtimeconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ctran88/powerlifting-app-v1.git"
  },
  "author": "Christopher Tran",
  "license": "SEE LICENSE IN Apache-2.0",
  "bugs": {
    "url": "https://github.com/ctran88/powerlifting-app-v1/issues"
  },
  "homepage": "https://github.com/ctran88/powerlifting-app-v1#readme",
  "devDependencies": {
    "firebase": "^4.1.3"
  }
}
